.g-user-list-entry
  padding 10px 12px
  margin 2px 0
  border-radius 5px
  background-color #f9fafa
  border 1px solid #d5dede

  .g-user-title
    font-size 17px

  .g-user-login
    color #666
    font-size 16px
    font-weight bold

  .g-space-used
    margin-top 2px

  .g-user-right-column
    border-left 1px solid #ddd
    padding-left 8px
    display inline
    width 210px
    font-size 14px
    float right
    color #909090

  .g-user-joined-date,.g-user-space-used
    color #444
